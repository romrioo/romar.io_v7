---
import '~/styles/styles.css';
import Layout from './Layout.astro';

type Props = {
  frontmatter: {
    title: string;
    pdfLink?: string;
    noIndex?: boolean;
    description?: string;
  };
};

const { title, pdfLink, noIndex, description } = Astro.props.frontmatter;

// SEO stuff
const ogImage = 'images/screenshot.png'; 
---
<Layout>
  <article class="region prose wrapper">

    <slot />
  </article>
</Layout>